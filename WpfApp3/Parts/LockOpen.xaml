<UserControl x:Class="WpfApp3.Parts.UserControl1"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:ed="http://schemas.microsoft.com/expression/2010/drawing"
             xmlns:local="clr-namespace:WpfApp3.Parts"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
        <Rectangle Width="200" Height="150" Fill="Transparent" HorizontalAlignment="Center"
                    VerticalAlignment="Bottom" RadiusX="10" RadiusY="10" Stroke="#CCCCC6"
                   StrokeThickness="8"/>
        <Grid Height="90" Width="130" Margin="20,0,0,80">
            <ed:Arc Stroke="#CCCCC6" StrokeThickness="8" StartAngle="-45" EndAngle="90"/>
        </Grid>
        <Ellipse Width="10" Height="10" Stroke="#CCCCC6" StrokeThickness="5" Margin="0,150,0,0"/>
    </Grid>
</UserControl>
